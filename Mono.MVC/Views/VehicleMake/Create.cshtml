@model Mono.MVC.Models.VehicleMakeViewModel;
<h1>Vehicle make </h1>
<form method="post">
   
<div class="mt-3">
    <label class="form-label">Name</label>
        <input type="text" class="form-control" required asp-for="Name" />
</div>
<div class="mt-3">
    <label class="form-label">Abrv</label>
        <input type="text" class="form-control" required asp-for="Abrv" />
</div>

<div class="mt-3">
    
    <button type="submit" class="btn btn-primary">Create</button>
</div>
    
</form>